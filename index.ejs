<!doctype html>
<html>
    <head>
        <meta charset="utf-8">
        <meta name="description" content="">
        <meta name="viewport" content="initial-scale=1">
        <title>Advertising</title>
        <link rel="stylesheet" href="projector.css">
        <link rel="stylesheet" href="projector-iframe.css">
    </head>
    <body>
    	<div class="ad"></div>

        <script src="projector.js"></script>
        <script>
            var el = document.querySelector('div.ad');

            var projector = new Projector(el, {
                columns: <%= columns %>,
                rows: <%= rows %>,
                frameRate: <%= framerate %>,
                totalFrames: <%= frames %>,
                dynamicQuality: true,
                quality: 0,
                imageFiles: 'final/%q-%i.jpg',
                movieUrl:   'video/compressed.mp4',
                audioUrl:   'audio/96-44.mp3',
                clickThroughUrl:   '',
                eventSrc:   '',
                events: [
                {
                    mark: '2s',
                    name: '2s'
                },
                {
                    mark: '1%',
                    name: '1%'
                },
                {
                    mark: '25%',
                    name: '25%'
                }, {
                    mark: '50%',
                    name: '50%'
                }, {
                    mark: '75%',
                    name: '75%'
                }, {
                    mark: '100%',
                    name: '100%'
                }]
            });

            projector.init();
        </script>


        
    </body>
</html>